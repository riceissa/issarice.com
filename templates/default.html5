<!DOCTYPE html>
<html$if(lang)$ lang="$lang$"$endif$$if(dir)$ dir="$dir$"$endif$>
<head prefix="og: http://ogp.me/ns# article: http://ogp.me/ns/article#">
  <meta charset="utf-8">
  <meta name="generator" content="pandoc">
  <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=yes">
$for(author-meta)$
  <meta name="author" content="$author-meta$">
$endfor$
$if(date-meta)$
  <meta name="dcterms.date" content="$date-meta$">
$endif$
$if(keywords)$
  <meta name="keywords" content="$for(keywords)$$keywords$$sep$, $endfor$">
$endif$
  <meta property="og:title" content="$pagetitle$" />
  <meta property="og:site_name" content="Issa Rice" />
  <meta property="og:locale" content="en_US" />
  <meta property="og:type" content="article" />
  <meta property="og:image" content="https://issarice.com/bird300.jpg" />
  <meta property="og:image:type" content="image/jpeg" />
  <meta property="og:image:width" content="300" />
  <meta property="og:image:height" content="300" />
  <meta property="article:author" content="https://www.facebook.com/riceissa" />
  <link rel="shortcut icon" type="image/ico" href="/favicon.ico" />
  <link rel="me" href="https://github.com/riceissa" />
  <link rel="me" href="https://twitter.com/riceissa" />
$if(bigtable)$
  <link rel="stylesheet" href="theme.default.css">
  <script src="jquery-latest.min.js"></script>
  <script src="jquery.tablesorter.js"></script>
$endif$
  <title>$if(title-prefix)$$title-prefix$ – $endif$$pagetitle$</title>
  <style type="text/css">code{white-space: pre;}</style>
$if(quotes)$
  <style type="text/css">q { quotes: "“" "”" "‘" "’"; }</style>
$endif$
$for(header-includes)$
$header-includes$
$endfor$
<style type="text/css">
  $if(css)$
$css$
  $endif$
</style>
$if(math)$
  $math$
$endif$
  <!--[if lt IE 9]>
    <script src="//cdnjs.cloudflare.com/ajax/libs/html5shiv/3.7.3/html5shiv-printshiv.min.js"></script>
  <![endif]-->
  <script src="anchor.min.js"></script>
  <script type="text/javascript">
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

    ga('create', 'UA-54798318-1', 'auto');
    ga('send', 'pageview');

  </script>
  <script src="change-theme.js"></script>
</head>
<body$if(bigtable)$ class="bigtable"$endif$>
<script>
  set_theme_from_cookies();
</script>
<nav id="site_navigation">
  $if(show-pandoc-template-comments)$
  <!--
    See
      http://stackoverflow.com/questions/37103/css-make-divs-align-horizontally
    for how the div elements are managed.
  -->
  $endif$
  <div style="float:left;">
    <a href="./index">Home</a>
    | <a href="./about">About</a>
    | <a href="./_all_date">Newest changes</a>
    $if(sourcefilename)$
    | <a href="https://raw.githubusercontent.com/riceissa/issarice.com/master/$sourcefilename$">View source</a>
    | <a href="https://github.com/riceissa/issarice.com/commits/master/$sourcefilename$">View history</a>
    $endif$
    <span id="changeThemeMenu" style="display: none;">| Change
    {<a href="#" onclick="change_theme_text_width()">text width</a>,
     <a href="#" onclick="change_theme_color()">color</a>,
     <a href="#" onclick="change_theme_font_family()">font</a>,
     <a href="#" onclick="change_theme_table()">table</a>}</span>
  </div>
  <div style="float:right;">
    $if(show-pandoc-template-comments)$
    <!--
      See
        - http://stackoverflow.com/a/13379065/3422337
        - http://kaspars.net/blog/wordpress/how-to-add-google-site-search
        - http://www.vanderbilt.edu/searchsupport/code-samples.php
    -->
    $endif$
    <form name="google-search" action="https://google.com/search" method="get">
      <fieldset style="border:0;">
        <input type="hidden" name="sitesearch" value="https://issarice.com" />
        <input name="q" type="text" />
        <input type="submit" name="btnG" value="Search site" />
      </fieldset>
    </form>
  </div>
</nav>

<main>
$if(title)$
<header>
<h1 class="title">$title$</h1>
$if(subtitle)$
<h1 class="subtitle">$subtitle$</h1>
$endif$

<dl class="page-metadata">
$if(created)$
  <dt>Creation date</dt>
  <dd>$created$</dd>
$endif$
$if(date)$
  <dt>Last substantive revision date</dt>
  <dd>$date$</dd>
$endif$
  <dt>Last modification date</dt>
$if(lastmodified)$
  <dd>$lastmodified$</dd>
$else$
  <dd>$today$</dd>
$endif$
$if(status)$
  <dt>Completion status</dt>
  <dd>$status$</dd>
$endif$
$if(belief)$
  <dt><a href="./belief">Belief</a></dt>
  <dd>$belief$</dd>
$endif$
</dl>

</header>
$endif$

$if(toc)$
<nav id="$idprefix$TOC">
<h2 id="TOCheading">Contents</h2>
$toc$
</nav>
$endif$
$body$
</main>

<footer>
<hr />
$if(license)$
$license$
$else$
<p>
  <a rel="license" href="https://creativecommons.org/publicdomain/zero/1.0/">
    <img src="./cc_public_domain.png" style="border-style: none;" alt="CC0" />
  </a>
  <br />
  To the extent possible under law, Issa Rice has waived all copyright and
  related or neighboring rights to the content on this page. This work is
  published from: United States.
  See the
  <a href="https://creativecommons.org/publicdomain/zero/1.0/">CC0
    1.0 Universal Public Domain Dedication</a>
  for more information.
</p>
$endif$
</footer>

<script>
  document.getElementById('changeThemeMenu').style.display='inline';
$if(bigtable)$
  $$(function(){
    $$("table").tablesorter();
  });
$endif$
  anchors.add();
</script>
</body>
</html>
